{
  "name": "minimal-expressapp",
  "version": "1.0.0",
  "description": "A minimal express app using Knex js and my Sql setup. Using HTTP Method and Postman as its minimal API gateway",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon src/index.js",
    "build": "node src/index.js",
    "migrate": "knex migrate:latest --knexfile src/knexmodels/knexfile.js",
    "migrate:down": "knex migrate:down --knexfile src/knexmodels/knexfile.js"
  },
  "author": "Teguh Hidayanto",
  "license": "MIT",
  "devDependencies": {
    "eslint": "latest",
    "eslint-config-airbnb-base": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-prettier": "latest",
    "nodemon": "latest",
    "prettier": "latest"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "latest",
    "dotenv": "latest",
    "express": "latest",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mysql2": "^3.9.7",
    "sequelize": "^6.6.5"
  }
}
